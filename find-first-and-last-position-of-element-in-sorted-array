class Solution {
    public int[] searchRange(int[] nums, int target) {
        ArrayList<Integer>ls=new ArrayList<Integer>();
        for(int num:nums)
        {
            ls.add(num);
        }
        int arr[]=new int[2];
        arr[0]=ls.indexOf(target);
        arr[1]=ls.lastIndexOf(target);
        return arr;
        
    }
}
